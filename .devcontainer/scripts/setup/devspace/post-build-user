export BROWSER=/usr/bin/microsoft-edge-beta &&
rcLine=export BROWSER=/usr/bin/microsoft-edge-beta &&
rcFile=$HOME/.bashrc &&
grep -qxF \"$rcLine\" \"$rcFile\" || echo \"$rcLine\" >> \"$rcFile\" &&
rcFile=$HOME/.zshrc &&
grep -qxF \"$rcLine\" \"$rcFile\" || echo \"$rcLine\" >> \"$rcFile\" &&
mkcert -install &&
dotnet dev-certs https --trust
