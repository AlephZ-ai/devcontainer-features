{
    "id": "dotnet",
    "version": "1.2.19",
    "name": "Dotnet CLI",
    "documentationURL": "https://github.com/AlephZ-ai/devcontainer-features/tree/main/src/dotnet",
    "description": "Installs the .NET CLI with preview, latest, and lts dotnet versions of the SDK and runtime.",
    "options": {
      "tools": {
        "type": "string",
        "default": "",
        "description": "Comma seperated list dotnet tools to install globally"
      }
    },
      "containerEnv": {
        "DOTNET_ROOT": "/usr/local/dotnet/current",
        "PATH": "/usr/local/dotnet/current:${PATH}"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-dotnettools.csharp"
            ]
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
