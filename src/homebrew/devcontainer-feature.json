{
    "name": "Homebrew",
    "id": "homebrew",
    "version": "3.2.13",
    "description": "Installs Homebrew",
    "documentationURL": "https://github.com/alephz-ai/devcontainer-features/tree/main/src/homebrew",
    "options": {
        "username": {
            "type": "string",
            "default": "automatic",
            "description": "User to install Homebrew"
        },
        "brews": {
            "type": "string",
            "default": "",
            "description": "Space seperated list of Brews to install after installing homebrew"
        }
    },
    "containerEnv": {
      "BREW_PREFIX": "/home/linuxbrew/.linuxbrew",
      "PATH": "/home/linuxbrew/.linuxbrew/bin:${PATH}"
    },
      "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils",
        "ghcr.io/devcontainers-contrib/featues/apt-get-packages",
        "ghcr.io/devcontainers-contrib/featues/apt-packages",
        "ghcr.io/rocker-org/devcontainer-features/apt-packages"
    ]
}
